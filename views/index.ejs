    <div class="row te-full">
      <h2 class="position-header">Tigh Ends</h2>
      <div class="col s6">
        <h2 class="centered">Josh</h2>
        <div class="table-wrap" id="tight-end-table">
          <table class="table-dragable">
            <thead>
            <tr>
              <th>Rank</th>
              <th>Name</th>
              <th>Team</th>
              <th>Tier</th>
              <th>ADP</th>
            </tr>
            </thead>
            <tbody>
            <% for(var i=0; i < te.length; i++) { %>
              <tr class="grabable tier<%= te[i]["Tier Josh"].toString() %> ">
              <td><%= te[i].Josh %></td>
                <td><%= te[i].Team  %></td>
                <td><%= te[i]["Tight Ends"] %></td>
                <td><%= te[i]["Tier Josh"] %> </td>
                <td class="adp"></td>
                <td class="grabable">&varr;</td>
              </tr>

            <%}%>
            </tbody>
          </table>
        </div>

      </div>

            <th>ADP</th>
          </tr>
          </thead>
          <tbody>
          <% for(var i=0; i < teBrandon.length; i++) { %>
          <%if (teBrandon[i]["Tier Brandon"] !== null && teBrandon[i]["Tier Brandon"] !== ''){%>
            <tr class="grabable tier<%= te[i]["Tier Josh"].toString() %> ">
              <td><%= teBrandon[i].Brandon %></td>
              <td><%= teBrandon[i].Team  %></td>
              <td><%= teBrandon[i]["Tight Ends"] %></td>

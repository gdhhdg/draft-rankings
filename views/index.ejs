    <h1 class="centered"><%= title %></h1>
    <div class="container">
      <row class="row">
        <div class="col s4 header-button" id="show-all-button">
          <a class="waves-effect waves-light btn">All</a>
        </div>
        <div class="col s4 header-button" id="show-te-button">
          <a class="waves-effect waves-light btn">Tight Ends</a>
        </div>
        <div class="col s4 header-button" id="show-qb-button">
          <a class="waves-effect waves-light btn">Quarterbacks</a>
        </div>
      </row>
      <row class="row">
        <div class="col s4 header-button" id="show-wr-button">
          <a class="waves-effect waves-light btn">Wide Receivers</a>
        </div>

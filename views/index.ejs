    <div class="row te-full">
      <h2 class="position-header">Tight Ends</h2>
      <div class="col s6">
        <h2 class="centered">Josh</h2>
        <div class="table-wrap" id="tight-end-table">
          <table id="josh-te-table" class="table-dragable">
            <thead>
            <tr>
              <th>Rank</th>
              <th>Name</th>
              <th>Team</th>
              <th>Tier</th>
              <th>ADP</th>
            </tr>
            </thead>
            <tbody>
            <% for(var i=0; i < teJosh.length; i++) { %>
              <%if (teJosh[i]["Tier Josh"] !== null && teJosh[i]["Tier Josh"] !== ''){%>
              <tr class="grabable tier<%= teJosh[i]["Tier Josh"].toString() %> ">
              <td><%= parseInt(teJosh[i].Josh %></td>
                <td><%= teJosh[i].Team  %></td>
                <td><%= teJosh[i]["Tight Ends"] %></td>
                <td><%= teJosh[i]["Tier Josh"] %> </td>
                <td class="adp"></td>
                <td class="grabable">&varr;</td>
              </tr>
            <%}%>
            <%}%>
            </tbody>
          </table>
        </div>

      </div>
      <div class="col s6">
        <h2 class="centered">Brandon</h2>
        <div class="table-wrap" id="tight-end-table">
